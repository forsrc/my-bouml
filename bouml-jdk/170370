    class 732034 "XIncludeParserConfiguration"
      visibility public 
      cpp_decl ""
      java_decl "${comment}${@}${visibility}${static}${final}${abstract}class ${name}${extends}${implements} {
${members}}
"
      php_decl ""
      python_2_2 python_decl ""
      idl_decl ""
      explicit_switch_type ""
      mysql_decl ""
      
      comment "This parser configuration includes an <code>XIncludeHandler</code> in the pipeline
before the schema validator, or as the last component in the pipeline if there is
