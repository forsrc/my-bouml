      extra_member 185730 "initialization"
	
	cpp "" ""
	java "  static {
        Object boldProperty = java.security.AccessController.doPrivileged(
            new GetPropertyAction(\"swing.boldMetal\"));
        if (boldProperty == null || !\"false\".equals(boldProperty)) {
            PLAIN_FONTS = false;
        }
        else {
            PLAIN_FONTS = true;
        }
    }"
	php ""
	python ""
	idl ""
	mysql ""
      end

