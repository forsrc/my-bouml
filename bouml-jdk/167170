      operation 4428290 "determineDocVersion"
	public explicit_return_type "short"
	nparams 1
	  param inout name "inputSource" type class_ref 657666 // XMLInputSource
	nexceptions 1
	  exception class_ref 146818 // IOException
	
	preserve_java_body_indent java_def "  ${comment}${@}${visibility}${final}${static}${abstract}${synchronized}${type} ${name}${(}${t0} ${p0}${)}${throws}${staticnl}{
  ${body}}
"
	
	
	
	comment "This methods scans the XML declaration to find out the version
(and provisional encoding)  of the document.
The scanning is doing using XML 1.1 scanner.
@param inputSource
@return short - Constants.XML_VERSION_1_1 if document version 1.1,
                 otherwise Constants.XML_VERSION_1_0
@throws IOException
"
      end

      operation 4428418 "fixupCurrentEntity"
	private explicit_return_type "void"
	nparams 3
	  param inout name "manager" type class_ref 667778 // XMLEntityManager
	  param inout name "scannedChars" explicit_type "char"
	multiplicity "[]"
	  param in name "length" explicit_type "int"
	
	preserve_java_body_indent java_def "  ${comment}${@}${visibility}${final}${static}${abstract}${synchronized}${type} ${name}${(}${t0} ${p0}, ${t1}[] ${p1}, ${t2} ${p2}${)}${throws}${staticnl}{
  ${body}}
"
	
	
	
	comment " This method prepends \"length\" chars from the char array,
 from offset 0, to the manager's fCurrentEntity.ch."
      end
    end
  end

