      operation 11522306 "parse"
	public explicit_return_type "void"
	nparams 5
	  param inout name "inputStream" type class_ref 146434 // InputStream
	  param inout name "factory" type class_ref 1363330 // DefaultSynthStyleFactory
	  param inout name "urlResourceBase" type class_ref 243458 // URL
	  param inout name "classResourceBase" type class_ref 130818 // Class
	  param inout name "defaultsMap" type class_ref 130178 // Map
	nexceptions 2
	  exception class_ref 372482 // ParseException
	  exception class_ref 187778 // IllegalArgumentException
	
	preserve_java_body_indent java_def "  ${comment}${@}${visibility}${final}${static}${abstract}${synchronized}${type} ${name}${(}${t0} ${p0}, ${t1} ${p1}, ${t2} ${p2}, ${t3}<?> ${p3}, ${t4}<String, Object> ${p4}${)}${throws}${staticnl}{
  ${body}}
"
	
	
	
	comment "Parses a set of styles from <code>inputStream</code>, adding the
resulting styles to the passed in DefaultSynthStyleFactory.
Resources are resolved either from a URL or from a Class. When calling
this method, one of the URL or the Class must be null but not both at
the same time.

@param inputStream XML document containing the styles to read
@param factory DefaultSynthStyleFactory that new styles are added to
@param urlResourceBase the URL used to resolve any resources, such as Images
@param classResourceBase the Class used to resolve any resources, such as Images
@param defaultsMap Map that UIDefaults properties are placed in
"
      end

