      operation 10269954 "getMidiFileFormat"
	class_operation public return_type class_ref 1250690 // MidiFileFormat
	nparams 1
	  param inout name "stream" type class_ref 146434 // InputStream
	nexceptions 2
	  exception class_ref 1249026 // InvalidMidiDataException
	  exception class_ref 146818 // IOException
	
